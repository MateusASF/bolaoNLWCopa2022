{
  "expo": {
    "name": "nlwcopamobile",
    "slug": "nlwcopamobile",
    "scheme": "nlwcopamobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-nlw.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-nlw.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-nlw.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon-nlw.png"
    }
  }
}
